<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff447de1" />
            <stroke android:width="5.0dip" android:color="#ff72a3f7" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff447de1" android:endColor="#ff4885ed" android:angle="270.0" />
            <stroke android:width="5.0dip" android:color="#ff72a3f7" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
</selector>